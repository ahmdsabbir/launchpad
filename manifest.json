{
  "manifest_version": 3,
  "name": "UniqueTab - Epic New Tab Launcher",
  "version": "0.0.0",
  "description": "Get an awesome new tab",
  "author": "Sabbir Ahmed <ahmd.sabbir@gmail.com>",
  "chrome_url_overrides" : {
    "newtab": "uniquetab.html"
  },
  "permissions": [
    "storage",
    "bookmarks"
  ],
  "icons": {
    "16": "assets/imgs/icon.png",
    "128": "assets/imgs/icon.png"
  }
}